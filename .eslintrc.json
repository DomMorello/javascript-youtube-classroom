{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:cypress/recommended"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "babelOptions": {
      "configFile": "./babel.config.json"
    }
  },
  "rules": {
    "import/extensions": ["error", "always"],
    "no-underscore-dangle": [
      "error",
      { "allowAfterThis": true, "allowAfterSuper": true }
    ]
  },
  "plugins": ["cypress"],
  "ignorePatterns": [
    "cypress/fixtures/*",
    "cypress/plugins/*",
    "cypress/support/*"
  ]
}
