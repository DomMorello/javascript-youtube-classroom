{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "ignorePatterns": ["node_modules/"],
  "rules": {
    "max-depth": 2,
    "prettier/prettier": "error",
    "max-lens": 80
  }
}
